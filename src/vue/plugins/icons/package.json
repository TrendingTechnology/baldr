{
  "name": "@bldr/icons",
  "version": "0.2.2",
  "description": "A selection of material design icons (https://materialdesignicons.com) extended with some custom icons.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josef-Friedrich/baldr.git"
  },
  "license": "MIT",
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "main": "src/plugin.js",
  "style": "src/style.css",
  "scripts": {
    "build": "vue-cli-service build",
    "build:browser": "tsc --module es2020 --outDir dist/",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ],
    "root": true
  },
  "dependencies": {
    "@bldr/themes": "*",
    "@bldr/vue-packages-bundler": "*"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "5.2.4",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "@vue/cli-plugin-babel": "4.5.9",
    "@vue/cli-plugin-e2e-cypress": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-router": "4.5.0",
    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-plugin-unit-mocha": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "chai": "4.1.2",
    "chalk": "4.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "node-sass": "5.0.0",
    "sass": "1.29.0",
    "sass-loader": "10.1.0",
    "style-resources-loader": "1.4.1",
    "typescript": "4.1.5",
    "vue-cli-plugin-style-resources-loader": "0.1.4",
    "vue-property-decorator": "9.1.2",
    "vue-template-compiler": "2.6.11",
    "webfont": "9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
