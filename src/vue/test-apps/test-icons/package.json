    "sass-loader": "10.1.0",
    "sass": "1.29.0",
    "style-resources-loader": "1.4.1",
    "typescript": "4.1.5",
    "vue-cli-plugin-style-resources-loader": "0.1.4",
    "vue-template-compiler": "2.6.11",
    "vue": "2.6.11"
