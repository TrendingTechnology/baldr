    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-plugin-unit-mocha": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "chai": "4.1.2",
    "chalk": "4.1.0",
    "eslint-plugin-vue": "6.2.2",
    "eslint": "6.7.2",
