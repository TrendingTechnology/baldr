{
  "name": "@bldr/test-icons",
  "version": "0.1.0",
  "description": "Test the Vue plugin @bldr/icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josef-Friedrich/baldr.git"
  },
  "license": "MIT",
