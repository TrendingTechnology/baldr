{
  "name": "@bldr/test-player",
  "description": "Test app for the Vue plugin @bldr/player",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:browser": "tsc --module es2020 --outDir dist/",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e:headless": "vue-cli-service test:e2e --headless",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "npm run test:e2e:headless",
    "lint": "vue-cli-service lint",
    "lint:ts-standard": "ts-standard --fix 'src/*.ts'"
  },
  "dependencies": {
    "@bldr/icons": "*",
    "@bldr/media-resolver-ng": "*",
    "@bldr/player": "*",
    "@bldr/string-format": "*",
    "vue-class-component": "7.2.3",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.2.0",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@bldr/config": "*",
    "@bldr/vue-config-helper": "*",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "eslint": "^6.7.2",
    "node-sass": "5.0.0",
    "sass-loader": "10.1.0",
    "sass": "1.29.0",
    "style-resources-loader": "1.4.1",
    "typescript": "~4.1.5",
    "vue-cli-plugin-style-resources-loader": "0.1.4",
    "vue-template-compiler": "2.6.11"
  },
  "main": "src/app",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": ["plugin:vue/essential", "@vue/typescript/recommended"],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": ["> 1%", "last 2 versions", "not dead"]
}
