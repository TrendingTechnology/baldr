{
  "files.exclude": {
    "*.log": true,
    "**/.env": true,
    "**/.eslintcache": true,
    "**/.npm": true,
    "**/.nyc_output": true,
    "**/*.log": true,
    "**/coverage": true,
    "**/jspm_packages/": true,
    "**/lib-cov": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/npm-debug.log*": true,
    "**/out": true,
    "**/package-lock.json": true,
    "**/pids": true,
    "**/typings/": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "javascript.validate.enable": true
}
